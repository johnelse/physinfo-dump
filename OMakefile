USE_OCAMLFIND = true

OCAMLFLAGS = -g -thread -bin-annot -warn-error +a-4-6-9-27-28-29

OCAMLPACKS = xenctrl

PROGRAM = physinfo_dump
FILES = physinfo_dump
.DEFAULT: $(OCamlProgram $(PROGRAM), $(FILES))

CLEAN_OBJS = *.a *.cmi *.cmo *.cmt *.cmti *.cmx *.cmxa *.o *.opt $(PROGRAM)
export CLEAN_OBJS

.PHONY: clean
clean:
	rm -f $(CLEAN_OBJS)
